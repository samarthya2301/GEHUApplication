<resources>
	<!-- Base application theme. -->
	<style name="Theme.GEHUApplication" parent="Theme.AppCompat.DayNight.DarkActionBar">
		<!-- Primary brand color. -->
		<item name="colorPrimary">@color/primary</item>
		<item name="colorPrimaryVariant">@color/primary_variant</item>
		<item name="colorOnPrimary">@color/primary_container</item>
		<!-- Secondary brand color. -->
		<item name="colorSecondary">@color/secondary</item>
		<item name="colorSecondaryVariant">@color/on_secondary_container</item>
		<item name="colorOnSecondary">@color/secondary_container</item>
		<!-- Status bar color. -->
		<item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>

		<!-- Customize your theme here. -->
		<item name="fontFamily">@font/ubuntu_bold</item>
	</style>

</resources>